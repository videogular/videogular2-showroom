{
    "project": {
        "version": "1.0.0-beta.24",
        "name": "videogular2-showroom"
    },
    "apps": [
        {
            "root": "src",
            "outDir": "dist",
            "assets": [
                "assets",
                "favicon.ico"
            ],
            "index": "index.html",
            "main": "main.ts",
            "test": "test.ts",
            "tsconfig": "tsconfig.json",
            "prefix": "app",
            "mobile": false,
            "styles": [
                "../node_modules/videogular2/fonts/videogular.css",
                "styles.css"
            ],
            "scripts": [
                "../node_modules/aframe/dist/aframe-v0.4.0.min.js",
                "../node_modules/aframe-animation-component/dist/aframe-animation-component.min.js",
                "../node_modules/aframe-bmfont-text-component/dist/aframe-bmfont-text-component.min.js",
                "../node_modules/dashjs/dist/dash.all.min.js",
                "../node_modules/hls.js/dist/hls.min.js"
            ],
            "environments": {
                "source": "environments/environment.ts",
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts"
            }
        }
    ],
    "addons": [],
    "packages": [],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "prefixInterfaces": false,
        "inline": {
            "style": false,
            "template": false
        },
        "spec": {
            "class": false,
            "component": true,
            "directive": true,
            "module": false,
            "pipe": true,
            "service": true
        }
    }
}
